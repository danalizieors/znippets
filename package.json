{
    "name": "znippets",
    "version": "0.1.0",
    "description": "Compress code snippets into shareable links",
    "repository": "git@github.com:danalizieors/znippets.git",
    "author": "Danalizi-Erőss Örs <danalizieors@gmail.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "format": "prettier --write .",
        "verify:compile": "tsc",
        "verify:format": "prettier --check .",
        "verify:test": "yarn test",
        "verify": "run-s verify:*",
        "test": "jest --config .jestrc.json --passWithNoTests",
        "start": "parcel source/index.html --cache-dir build/cache --out-dir build/start",
        "build:local": "parcel build source/index.html --cache-dir build/cache --out-dir build/local",
        "build:deploy": "parcel build source/index.html --cache-dir build/cache --out-dir build/deploy --public-url /znippets/",
        "build": "run-p build:*",
        "clean": "rm -rf build",
        "serve": "serve --listen 1234 build/local"
    },
    "dependencies": {
        "@chakra-ui/react": "^1.3.4",
        "@emotion/react": "^11.1.5",
        "@emotion/styled": "^11.1.5",
        "fflate": "^0.6.9",
        "framer-motion": "^3.10.5",
        "js-base64": "^3.6.0",
        "ramda": "^0.27.1",
        "react": "^17.0.1",
        "react-codejar": "^1.0.2",
        "react-dom": "^17.0.1",
        "react-icons": "^4.2.0",
        "react-router-dom": "^5.2.0",
        "zustand": "^3.3.3"
    },
    "devDependencies": {
        "@types/jest": "^26.0.21",
        "@types/ramda": "^0.27.39",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/react-router-dom": "^5.1.7",
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.5",
        "prettier": "^2.2.1",
        "prettier-plugin-organize-imports": "^1.1.1",
        "serve": "^11.3.2",
        "ts-jest": "^26.5.4",
        "typescript": "^4.2.3"
    }
}
